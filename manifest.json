{
  "name": "ColumnCopy",
  "version": "0.5.1",
  "manifest_version": 2,
  "description": "Enables copying columns from tables.Supports Column Seperator options.",
  "homepage_url": "https://github.com/jamesandres/ColumnCopy",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "permissions": [
    "clipboardWrite",
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": [
        "file://*/*",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/jquery/jquery.js",
        "js/jquery.hotkeys/jquery.hotkeys.js",
        "src/inject/inject.js"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/jquery/jquery.js",
    "js/jquery/jquery.min.map"
  ]
}
